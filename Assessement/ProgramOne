package evaluation;

public class ProgramOne {

	public static void main(String[] args) {
		int[] arr = { 2, 0, 1, 0, 4, 0 };

		for (int i = 0; i < arr.length - 1; i++) {
			if (arr[i] ==0 && arr[i+1]>0) {
				int temp = arr[i];
				arr[i] = arr[i + 1];
				arr[i + 1] = temp;
			}
		}

		for (int eachValue : arr) {
			System.out.print(eachValue);
		}
	}

}
