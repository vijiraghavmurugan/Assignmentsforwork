package evaluation;

public class ProgramOne {

	public static void main(String[] args) {
		int[] arr = { 2, 0, 1, 0, 4, 0, 5, 9, 0 };
		int[] arrCopy = new int[arr.length];

		int count = 0;
		for (int i = 0; i < arr.length; i++) {
			if (arr[i] > 0) {
				arrCopy[count] = arr[i];
				count++;
			}

		}

		for (int eachValue : arrCopy) {
			System.out.print(eachValue);
		}
	}

}
